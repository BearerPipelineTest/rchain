
new
  updateContract,
  AuthKey,
  AuthStateCh,
  rl(`rho:registry:lookup`),
  rlret,
  _authKey,
  stdout(`rho:io:stdout`),
  updateRet
in {
  rl!(`rho:registry:systemContractManager`, *rlret)|
  for(@systemContractManager <- rlret){
    stdout!(("update get multisig ok", systemContractManager))|
    @systemContractManager!("update", `rho:rchain:authKey`, *updateRet)|
    for (@result <- updateRet){
      stdout!((result, "update result "))
    }
  }|
  stdout!(("update start!!!!!"))

}